<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hh.reggie.mapper.OrderDetailMapper">
    <insert id="saveBatch" parameterType="java.util.List">
        inser into order_detail(id,name,image,order_id,dish_id,setmeal_id,dish_flavor,number,amount)
        values
        <foreach collection="list" separator="," item="com.hh.reggie.entity.OrderDetail">
            (#{OrderDetail.id},#{OrderDetail.name},#{OrderDetail.image},#{OrderDetail.orderId},#{OrderDetail.dishId},#{OrderDetail.setmealId},#{OrderDetail.dishFlavor},#{OrderDetail.number},#{OrderDetail.amount})
        </foreach>
    </insert>
</mapper>
