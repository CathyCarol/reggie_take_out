<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hh.reggie.mapper.OrderMapper">
    <insert id="save" parameterType="com.hh.reggie.entity.Orders">
        insert into orders(id,number,status,user_id,address_book_id,order_time,checkout_time,amount,remark,phone,address,user_name,consignee)
        values(#{id},#{number},#{status},#{userId},#{addressBookId},#{orderTime},#{checkoutTime},#{amount},#{remark},#{phone},#{address},#{userName},#{consignee})
    </insert>
</mapper>
