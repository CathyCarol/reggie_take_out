<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hh.reggie.mapper.DishFlavorMapper">
    <insert id="saveBatch" parameterType="list">
        insert into dish_flavor(name,value) values
        <foreach collection="list" separator="," item="com.hh.reggie.dto.DishDto">
        (#{DishDto.name},#{DishDto.value})
        </foreach>
    </insert>
</mapper>
